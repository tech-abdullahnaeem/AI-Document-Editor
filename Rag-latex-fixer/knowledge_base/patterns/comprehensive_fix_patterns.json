{
  "automated_fix_patterns": {
    "unicode_character_fixes": {
      "pattern_type": "character_replacement",
      "detection_method": "unicode_scanning",
      "fixes": [
        {
          "pattern": "α",
          "replacement": "$\\\\alpha$",
          "context_check": "not_in_math_mode",
          "confidence": 0.95
        },
        {
          "pattern": "β", 
          "replacement": "$\\\\beta$",
          "context_check": "not_in_math_mode",
          "confidence": 0.95
        },
        {
          "pattern": "≤",
          "replacement": "$\\\\leq$", 
          "context_check": "not_in_math_mode",
          "confidence": 0.98
        },
        {
          "pattern": "≥",
          "replacement": "$\\\\geq$",
          "context_check": "not_in_math_mode", 
          "confidence": 0.98
        },
        {
          "pattern": "\u201c",
          "replacement": "``",
          "context_check": "start_of_quotation",
          "confidence": 0.90
        },
        {
          "pattern": "\u201d",
          "replacement": "''",
          "context_check": "end_of_quotation",
          "confidence": 0.90
        },
        {
          "pattern": "—",
          "replacement": "---",
          "context_check": "always",
          "confidence": 0.95
        },
        {
          "pattern": "…",
          "replacement": "\\\\ldots",
          "context_check": "always",
          "confidence": 0.98
        }
      ]
    },
    "math_mode_fixes": {
      "pattern_type": "environment_wrapping",
      "detection_method": "context_analysis",
      "fixes": [
        {
          "pattern": "mathematical_symbols_outside_math",
          "symbols": ["∑", "∏", "∫", "∂", "∇", "∞"],
          "action": "wrap_in_math_mode",
          "wrapper": "${}$",
          "confidence": 0.95
        },
        {
          "pattern": "greek_letters_in_text",
          "symbols": ["α", "β", "γ", "δ", "ε", "ζ", "η", "θ", "λ", "μ", "π", "σ", "τ", "φ", "χ", "ψ", "ω"],
          "action": "wrap_in_math_mode",
          "wrapper": "${}$", 
          "confidence": 0.90
        },
        {
          "pattern": "subscript_superscript_outside_math",
          "detection": "regex",
          "regex": "\\\\w+[_^]\\\\w+",
          "action": "wrap_in_math_mode",
          "confidence": 0.85
        }
      ]
    },
    "table_formatting_fixes": {
      "pattern_type": "structural_replacement",
      "detection_method": "environment_analysis",
      "fixes": [
        {
          "pattern": "hline_to_booktabs",
          "detection": "\\\\hline",
          "replacements": {
            "first_hline": "\\\\toprule",
            "middle_hline": "\\\\midrule", 
            "last_hline": "\\\\bottomrule"
          },
          "requires_package": "booktabs",
          "confidence": 0.95
        },
        {
          "pattern": "remove_vertical_lines",
          "detection": "\\\\begin{tabular}{.*\\\\|.*}",
          "action": "remove_pipe_characters",
          "confidence": 0.90
        },
        {
          "pattern": "optimize_column_alignment",
          "detection": "table_content_analysis",
          "action": "analyze_and_suggest_alignment",
          "confidence": 0.80
        }
      ]
    },
    "citation_fixes": {
      "pattern_type": "reference_standardization",
      "detection_method": "citation_pattern_analysis",
      "fixes": [
        {
          "pattern": "inconsistent_citation_style",
          "detection": "mixed_citation_formats",
          "action": "standardize_to_dominant_style",
          "confidence": 0.75
        },
        {
          "pattern": "missing_citations",
          "detection": "factual_claims_without_references",
          "action": "flag_for_manual_review",
          "confidence": 0.60
        },
        {
          "pattern": "malformed_bibtex_entries",
          "detection": "bibtex_syntax_check",
          "action": "fix_bibtex_formatting",
          "confidence": 0.85
        }
      ]
    },
    "figure_fixes": {
      "pattern_type": "media_optimization",
      "detection_method": "figure_analysis",
      "fixes": [
        {
          "pattern": "missing_file_extensions",
          "detection": "includegraphics_without_extension",
          "action": "add_appropriate_extension",  
          "confidence": 0.70
        },
        {
          "pattern": "inappropriate_figure_format",
          "detection": "eps_with_pdflatex",
          "action": "suggest_format_conversion",
          "confidence": 0.90
        },
        {
          "pattern": "missing_figure_dimensions",
          "detection": "includegraphics_without_size",
          "action": "add_width_specification",
          "default_width": "0.8\\\\textwidth",
          "confidence": 0.80
        }
      ]
    },
    "package_fixes": {
      "pattern_type": "dependency_resolution",
      "detection_method": "error_log_analysis",
      "fixes": [
        {
          "pattern": "missing_package_for_command",
          "command_to_package_map": {
            "\\\\toprule": "booktabs",
            "\\\\includegraphics": "graphicx",
            "\\\\cite": "natbib or biblatex",
            "\\\\url": "url",
            "\\\\hyperref": "hyperref",
            "\\\\SI": "siunitx",
            "\\\\begin{algorithm}": "algorithm"
          },
          "action": "add_usepackage",
          "confidence": 0.95
        },
        {
          "pattern": "package_option_conflicts",
          "detection": "option_clash_error",
          "action": "resolve_package_options",
          "confidence": 0.80
        }
      ]
    }
  },
