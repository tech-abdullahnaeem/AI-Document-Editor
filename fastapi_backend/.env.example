# FastAPI Backend Environment Variables

# API Keys
FASTAPI_API_KEY=your-secret-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
GOOGLE_API_KEY=your-gemini-api-key-here
MATHPIX_APP_ID=your-mathpix-app-id-here
MATHPIX_APP_KEY=your-mathpix-app-key-here

# Server Configuration
HOST=0.0.0.0
PORT=8000
RELOAD=True
LOG_LEVEL=info

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# File Storage
MAX_FILE_SIZE_MB=50
CLEANUP_INTERVAL_HOURS=24

# LaTeX Compilation
DEFAULT_LATEX_ENGINE=pdflatex
COMPILATION_TIMEOUT_SECONDS=60
